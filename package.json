{
  "name": "event-hero-client-v2",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "prepare": "husky install",
    "format": "prettier --config .prettierrc.cjs --write '**/*.{js,jsx,ts,tsx,json,md,css,graphql,prettierrc,yml}'",
    "check-format": "prettier --config .prettierrc.cjs --check '**/*.{js,jsx,ts,tsx,json,md,css,graphql,prettierrc,yml}'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.15",
    "@mui/material": "^5.11.0",
    "axios": "^1.2.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "react-toastify": "^9.1.1",
    "tss-react": "^4.4.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.8.4",
    "typescript": "*",
    "vite": "^4.1.1"
  },
  "lint-staged": {
    "*{ts,tsx}": [
      "yarn format",
      "eslint --cache --fix"
    ]
  }
}
